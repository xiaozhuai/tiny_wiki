<IfModule mod_rewrite.c>
	RewriteEngine On
	## if you enable the force redirect
	## You need to add your book root dir to the blow, cause request to imgs or other files in book will each 403
	## here came out sample_book
	RewriteRule ^(?!theme/|sample_book/|favicon.ico).*$ index.php
</IfModule>

